{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "plugins": ["prettier"],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
      "no-unused-vars":"warn",
      "no-console": "warn",
      "func-names": "off",
      "object-shorthand": "off",
      "no-underscore-dangle": "off"
    }
}
